<h1>To Do List</h1>
<ul>
  @foreach(var task in Model)
  {
    <li><a href='/tasks/@task.GetID()'>@task.GetDescription()</a></li>
  }
</ul>
<p><a href='/tasks/new'>Add new task</a></p>
